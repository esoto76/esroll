{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "allowJs": true,
    "noEmitOnError": true,
    "types": ["@types/node"],
    "jsx": "react"
  },
  "exclude": ["node_modules"]
}
